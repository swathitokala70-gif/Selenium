<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="3" parallel="tests">
<listeners>
<listener class-name="GenericUtilities.ListernersUtility"></listener>
</listeners>
  <test name="Test_chrome">
  <parameter name="browser" value="Chrome"></parameter>
    <classes>
      <class name="Contact_Module.Contact_scenarios_Test"/>
      <class name="Organization_Module.Organization_scenarios_Test"/>
    </classes>
  </test> <!-- Test -->
   <test name="Test_edge">
     <parameter name="browser" value="Edge"></parameter>
    <classes>
      <class name="Contact_Module.Contact_scenarios_Test"/>
      <class name="Organization_Module.Organization_scenarios_Test"/>
    </classes>
  </test> <!-- Test -->
   <test name="Test_firefox">
    <parameter name="browser" value="Firefox"></parameter>
    <classes>
      <class name="Contact_Module.Contact_scenarios_Test"/>
      <class name="Organization_Module.Organization_scenarios_Test"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
